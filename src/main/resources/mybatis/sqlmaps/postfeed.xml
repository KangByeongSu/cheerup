<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="postfeed">

	<insert id="insertFeed" parameterType="com.skplanet.project2.model.PostFeedDTO">
	insert into Elephant_Contents(Elephant_User_u_id,c_comment,c_imgurl,c_time)
	values(#{userId},#{comment},#{imgUrl},CURRENT_TIMESTAMP());
	</insert>
	
	
	
</mapper>